<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:padding="6dp"
                    android:text="默认" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:padding="6dp"
                    android:text="更改" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="imei：" />

                <TextView
                    android:id="@+id/tv_imei_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_imei_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_imei"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="androidID：" />

                <TextView
                    android:id="@+id/tv_ID_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_ID_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_ID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="wifi mac：" />

                <TextView
                    android:id="@+id/tv_wifi_mac_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_wifi_mac_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_wifi_mac"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="wifi ssid：" />

                <TextView
                    android:id="@+id/tv_wifi_ssid_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_wifi_ssid_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_wifi_ssid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="手机号：" />

                <TextView
                    android:id="@+id/tv_phone_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_phone_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="sim iccid：" />

                <TextView
                    android:id="@+id/tv_sim_iccid_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_sim_iccid_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_sim_iccid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="sim IMSI：" />

                <TextView
                    android:id="@+id/tv_sim_IMSI_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_sim_IMSI_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_sim_IMSI"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="LocalWIFIIpAdress：" />

                <TextView
                    android:id="@+id/tv_LocalWIFIIpAdress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_LocalWIFIIpAdress_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_LocalWIFIIpAdress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="GPRSLocalIpAddress：" />

                <TextView
                    android:id="@+id/tv_GPRSLocalIpAddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_GPRSLocalIpAddress_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_GPRSLocalIpAddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="other：" />

                <TextView
                    android:id="@+id/tv_other_default"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789" />

                <TextView
                    android:id="@+id/tv_other_modify"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="4dp"
                    android:text="123456789" />

                <Button
                    android:id="@+id/bt_other"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="change" />

            </LinearLayout>

        </LinearLayout>

    </ScrollView>




</LinearLayout>